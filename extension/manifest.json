{
   "background": {
      "scripts": [ "scripts/jquery.js", "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/yhack_48.png"
   },
   "content_scripts": [ {
      "js": [ "scripts/jquery.js", "scripts/contentscript.js", "scripts/shadowbox.js", "scripts/jquery.transit.min.js" ],
      "css": [ "styles/yhack.css", "styles/shadowbox.css" ],
      "matches": ["*://*.facebook.com/*"]
   } ],
   "manifest_version": 2,
   "description": "Yahoo! Open Hack Day 2012 - Ya!hook",
   "icons": {
      "128": "icons/yhack_128.png",
      "48": "icons/yhack_48.png",
      "16": "icons/yhack_16.png"
   },
   "name": "Ya!hook",
   "permissions": [ "tabs", "*://www.facebook.com/","http://*/"],
   "version": "1.0.0",
   "web_accessible_resources": [
      "img/close.png",
      "img/next.png",
      "img/previous.png",
      "img/play.png",
      "img/pause.png",
      "img/loading.gif",
      "icon/yhack_facebook_48.png",
      "img/hook_resize.png",
      "img/ya_resize.png"
  ]
}
